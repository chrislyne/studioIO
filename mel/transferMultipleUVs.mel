global proc transferMultipleUVs()
{
	string $sel[] = `ls -sl`;
	for( $item in $sel)
	{

	  transferAttributes -transferPositions 0 -transferNormals 0 -transferUVs 2 -transferColors 2 -sampleSpace 4 -sourceUvSpace "map1" -targetUvSpace "map1" -searchMethod 3-flipUVs 0 -colorBorders 1 $sel[0] $item;
	}
}